		{
			"name": "Login",
			"item": [
				{
					"name": "Login Request API",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"let smsession = pm.cookies.get('SMSESSION');",
									"pm.environment.set('ltpaToken2',pm.cookies.get('LtpaToken2'));",
									"pm.environment.set('smsession',smsession);",
									"pm.test(\"Login API Status\", function () {",
									"    pm.response.to.have.status(200);",
									"});"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [],
						"url": {
							"raw": "{{login-api-host}}/forms/RWO/OEDMZLogin.fcc?smauthreason=0&target={{target}}/eps/mycah&SMENC=ISO-8859-1&PASSWORD={{password}}&smagentname={{smagentname}}&postpreservationdata=&smquerydata=&SMLOCALE=US-EN&REMEMBER=on&USER={{user}}",
							"host": [
								"{{login-api-host}}"
							],
							"path": [
								"forms",
								"RWO",
								"OEDMZLogin.fcc"
							],
							"query": [
								{
									"key": "smauthreason",
									"value": "0"
								},
								{
									"key": "target",
									"value": "{{target}}/eps/mycah"
								},
								{
									"key": "SMENC",
									"value": "ISO-8859-1"
								},
								{
									"key": "PASSWORD",
									"value": "{{password}}"
								},
								{
									"key": "smagentname",
									"value": "{{smagentname}}"
								},
								{
									"key": "postpreservationdata",
									"value": ""
								},
								{
									"key": "smquerydata",
									"value": ""
								},
								{
									"key": "SMLOCALE",
									"value": "US-EN"
								},
								{
									"key": "REMEMBER",
									"value": "on"
								},
								{
									"key": "USER",
									"value": "{{user}}"
								}
							]
						}
					},
					"response": []
				},
				{
					"name": "Token Generator API",
					"event": [
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									""
								],
								"type": "text/javascript"
							}
						},
						{
							"listen": "test",
							"script": {
								"exec": [
									"let response = pm.response.json();",
									"let Authorization = response.access_token;",
									"pm.environment.set('Authorization',Authorization);",
									"pm.test(\"Token Generator API Status\", function () {",
									"    pm.response.to.have.status(200);",
									"});"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"auth": {
							"type": "noauth"
						},
						"method": "GET",
						"header": [
							{
								"key": "sm-session",
								"type": "text",
								"value": "{{smsession}}"
							},
							{
								"key": "x-api-key",
								"type": "text",
								"value": "{{x-api-key}}"
							},
							{
								"key": "Content-Type",
								"type": "text",
								"value": "application/json"
							},
							{
								"key": "WCToken",
								"type": "text",
								"value": "{{WCToken}}",
								"disabled": true
							}
						],
						"url": {
							"raw": "{{token-generator-api-host}}/{{env}}/api/v1/token-generator/application/mobile/generate?user={{user}}",
							"host": [
								"{{token-generator-api-host}}"
							],
							"path": [
								"{{env}}",
								"api",
								"v1",
								"token-generator",
								"application",
								"mobile",
								"generate"
							],
							"query": [
								{
									"key": "user",
									"value": "{{user}}"
								}
							]
						}
					},
					"response": []
				}
			]
		},
